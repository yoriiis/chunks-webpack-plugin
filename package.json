{
	"name": "chunks-webpack-plugin",
	"version": "8.0.0",
	"description": "Create HTML files with entrypoints and chunks relations to serve your bundles",
	"keywords": [
		"chunks",
		"chunks-webpack-plugin",
		"plugin",
		"split chunks",
		"webpack"
	],
	"homepage": "https://github.com/yoriiis/chunks-webpack-plugin",
	"bugs": "https://github.com/yoriiis/chunks-webpack-plugin/issues",
	"repository": "https://github.com/yoriiis/chunks-webpack-plugin.git",
	"funding": {
		"type": "individual",
		"url": "https://github.com/sponsors/yoriiis"
	},
	"license": "MIT",
	"author": "Joris DANIEL",
	"main": "./lib/index.js",
	"types": "./types/index.d.ts",
	"files": [
		"lib",
		"types"
	],
	"scripts": {
		"analyze:bundle": "webpack --config=./example/webpack.config.js --mode=production --profile --json > webpack-stats.json && webpack-bundle-analyzer ./webpack-stats.json ./ -p 1664",
		"build": "rm -rf ./types ./lib && tsc",
		"build:example": "rm -rf ./example/dist/ && webpack --config=./example/webpack.config.js --mode=production",
		"coverage": "jest --config ./config/jest.config.js --coverage && cat ./coverage/lcov.info | coveralls",
		"dev": "tsc --watch",
		"dev:unit": "jest --config ./config/jest.config.js --watchAll --verbose --coverage",
		"inspect:example": "rm -rf ./example/dist/ && node --inspect node_modules/webpack/bin/webpack.js --config=./example/webpack.config.js --mode=development",
		"pre:publish": "npm run build && npm run build:example && npm run test",
		"start:example": "rm -rf ./example/dist/ && webpack --config=./example/webpack.config.js --mode=development",
		"test": "npm run test:eslint && npm run test:types && npm run test:markdown && npm run test:unit && npm run test:mutation",
		"test:eslint": "eslint . -c ./config/.eslintrc.js --ignore-pattern coverage --ignore-pattern reports --ignore-pattern example/dist --ignore-pattern lib --ignore-pattern types",
		"test:markdown": "markdownlint '**/*.md' --ignore node_modules",
		"test:mutation": "stryker run",
		"test:types": "tsc --noEmit",
		"test:unit": "jest --config ./config/jest.config.js --verbose --coverage"
	},
	"prettier": "./config/prettier.config.js",
	"dependencies": {
		"schema-utils": "^4.0.1"
	},
	"devDependencies": {
		"@babel/core": "^7.21.4",
		"@babel/preset-env": "^7.21.4",
		"@babel/preset-typescript": "^7.21.4",
		"@stryker-mutator/babel-transpiler": "^4.0.0",
		"@stryker-mutator/core": "^6.4.2",
		"@stryker-mutator/javascript-mutator": "^4.0.0",
		"@stryker-mutator/jest-runner": "^6.4.2",
		"@stryker-mutator/typescript": "^4.0.0",
		"@stryker-mutator/webpack-transpiler": "^4.0.0",
		"@types/fs-extra": "^11.0.1",
		"@types/jest": "^29.5.1",
		"@types/node": "^18.15.11",
		"@types/tapable": "^2.2.3",
		"@types/webpack": "^5.28.1",
		"@typescript-eslint/eslint-plugin": "^5.59.0",
		"@typescript-eslint/parser": "^5.59.0",
		"babel-loader": "^9.1.2",
		"babel-plugin-dynamic-import-node": "^2.3.3",
		"coveralls": "^3.1.1",
		"css-loader": "^6.7.3",
		"css-minimizer-webpack-plugin": "^5.0.0",
		"eslint": "^8.38.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-config-standard": "^17.0.0",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-n": "^15.7.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-promise": "^6.1.1",
		"jest": "^29.5.0",
		"markdownlint-cli": "^0.33.0",
		"mini-css-extract-plugin": "^2.7.5",
		"prettier-eslint": "^15.0.1",
		"sanitize.css": "^13.0.0",
		"terser-webpack-plugin": "^5.3.7",
		"ts-jest": "^29.1.0",
		"typescript": "^5.0.4",
		"vlitejs": "^4.2.0",
		"webpack": "^5.80.0",
		"webpack-bundle-analyzer": "^4.8.0",
		"webpack-cli": "^5.0.1"
	},
	"peerDependencies": {
		"webpack": "^5.0.0"
	},
	"engines": {
		"node": ">=16.20.0"
	}
}
