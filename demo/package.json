{
	"name": "chunks-webpack-plugin",
	"version": "1.0.0",
	"description": "ChunksWebpackPlugin demo with a minimalist project",
	"scripts": {
		"analyze:bundle": "webpack --mode=production --profile --json > webpack-stats.json && webpack-bundle-analyzer ./webpack-stats.json ./ -p 1664",
		"build": "rm -rf ./dist/ && webpack --mode=production",
		"inspect": "rm -rf ./dist/ && node --inspect node_modules/webpack/bin/webpack.js --mode=production --hidden=node_modules/",
		"start": "rm -rf ./dist/ && webpack --mode=development"
	},
	"dependencies": {
		"sanitize.css": "11.0.0",
		"vlitejs": "3.0.2"
	},
	"devDependencies": {
		"babel-loader": "8.0.6",
		"css-loader": "1.0.0",
		"mini-css-extract-plugin": "0.4.1",
		"optimize-css-assets-webpack-plugin": "^5.0.3",
		"terser-webpack-plugin": "^2.2.1",
		"webpack": "4.41.2",
		"webpack-bundle-analyzer": "3.6.0",
		"webpack-cli": "3.3.10",
		"webpack-manifest-plugin": "2.2.0"
	},
	"engines": {
		"node": ">=8.11.2"
	}
}
